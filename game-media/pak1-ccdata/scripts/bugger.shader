models/players/bugger/espaersc
{
	{
		map models/players/bugger/espaerscfx.jpg
		rgbGen lightingDiffuse
		tcMod scroll 7 1.2
	}
        {
		map models/players/bugger/espaersc.tga
                blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen lightingDiffuse
	} 
        {
		map $lightmap
                blendFunc GL_DST_COLOR GL_ONE_MINUS_DST_ALPHA
		rgbGen lightingDiffuse
	}
}

models/players/bugger/sezriryl
{
