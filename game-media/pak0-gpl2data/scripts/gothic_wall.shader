
textures/gothic_wall/iron01_f
{
	qer_editorimage textures/gothic_wall/iron01_f.tga
	{
		map $lightmap
		rgbgen identity      
	}
	
	{
		map textures/gothic_wall/iron01_f.tga
		blendFunc GL_DST_COLOR GL_SRC_ALPHA
		rgbGen identity
		alphaGen lightingSpecular
	}
}

