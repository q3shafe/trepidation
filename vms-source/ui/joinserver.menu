#include "ui/menudef.h"

{
\\ Server Join \\

  menuDef {
    	name "joinserver"
    	visible 0
    	fullscreen 1
    	rect 0 0 640 480
	background "menuback_a"
	focusColor 1 .75 0 1
	outOfBoundsClick	
    	style 1
    	border 1
	onOpen { setitemcolor fadebox backcolor 0 0 0 1 ; 
	playlooped "music/fla_mp03.wav" ;
	fadeout fadebox ; 
	uiScript UpdateFilter ;
	hide lightning ; 
	hide accept_alt ; 
	show accept ;
	hide back_alt ;
	show back ;
	hide grpmessage }
	onEsc { uiScript closeJoin }


